<?xml version="1.0"?>
<calendars>
    <calendar id="saint">
        <seed>0</seed>
        <milestones order="strict">year, month, day, unifiedTime</milestones>
        <units>
            <unit id="year">GS</unit>
            <unit id="month" multiple="true">ASC, DESC</unit>
            <unit id="day" multiple="true">BR, EX</unit>
        </units>
        <configurations>
            <configuration id="year">
                <mapping id="day">100</mapping>
                <mapping id="month">2</mapping>
            </configuration>
            <configuration id="month">
                <mapping id="day">50</mapping>
            </configuration>
            <configuration id="day">
                <mapping id="unifiedTime">600</mapping>
            </configuration>
       </configurations>
    </calendar>
</calendars>
